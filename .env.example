APP_ENV=development
APP_DEBUG=true

# Binance API keys (opcional para endpoints públicos)
BINANCE_API_KEY=
BINANCE_SECRET_KEY=

# Base URL (use testnet ou override completo)
# BINANCE_TESTNET=true
# BINANCE_BASE_URL=https://testnet.binance.vision

# SSL
# BINANCE_SSL_VERIFY=true
# BINANCE_CA_BUNDLE=/caminho/para/cacert.pem

# Proteção básica das rotas (opcional)
# BASIC_AUTH_USER=
# BASIC_AUTH_PASSWORD=

# Rate limit (opcional; aplica a account/trading)
# RATE_LIMIT_ENABLED=false
# RATE_LIMIT_MAX=60
# RATE_LIMIT_WINDOW=60

# Logging (opcional; usa JSON, depende do APP_DEBUG)
# APP_LOG_FILE=./storage/logs/app.log

# Cache
# CACHE_EXCHANGEINFO_TTL=30
