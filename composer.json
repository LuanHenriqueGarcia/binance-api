{
  "name": "binance/api-rest-php",
  "description": "API REST em PHP para integração com Binance - Sem dependências externas",
  "type": "project",
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.3",
    "phpstan/phpstan": "^1.11"
  },
  "autoload": {
    "psr-4": {
      "BinanceAPI\\": "src/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "stan": "vendor/bin/phpstan analyse",
    "serve": "php -S localhost:8000 -t ."
  }
}
